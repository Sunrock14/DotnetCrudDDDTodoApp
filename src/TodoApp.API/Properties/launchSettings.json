    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Karşıki dağlar jandarma": {
      "commandName": "Project",
      "launchBrowser": true,
